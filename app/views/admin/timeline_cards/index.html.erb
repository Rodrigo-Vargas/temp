<h2>Timeline Cards</h2>

<table class="table table-stripped">
  <% @cards.each do | card | %>
    <tr>
      <td><%= card.name %></td>
      <td>
        <%= link_to edit_admin_timeline_card_path(card) do %>
          <i class="fa fa-pencil"></i>
        <% end %>
      </td>
      <td>
        <%= link_to [:admin, card], data: { confirm: 'Are you sure?'}, method: :delete do %>
          <i class="fa fa-trash"></i>
        <% end %>
      </td>
    </tr>
  <% end %>
</table>